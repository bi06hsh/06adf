{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory-Name",
			"defaultValue": "df2bi06"
		},
		"IsStar_connectionString": {
			"type": "secureString",
			"metadata": "Sichere Zeichenfolge für \"connectionString\" von \"IsStar\""
		},
		"isDV_connectionString": {
			"type": "secureString",
			"metadata": "Sichere Zeichenfolge für \"connectionString\" von \"isDV\""
		},
		"isNorthwind_connectionString": {
			"type": "secureString",
			"metadata": "Sichere Zeichenfolge für \"connectionString\" von \"isNorthwind\""
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI für Speicherkonto mit verknüpften ARM-Vorlagen"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "SAS-Token für Speicherkonto mit verknüpften ARM-Vorlagen"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df2bi06_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"IsStar_connectionString": {
						"value": "[parameters('IsStar_connectionString')]"
					},
					"isDV_connectionString": {
						"value": "[parameters('isDV_connectionString')]"
					},
					"isNorthwind_connectionString": {
						"value": "[parameters('isNorthwind_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}